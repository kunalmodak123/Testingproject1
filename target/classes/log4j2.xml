<?xml version="1.0" encoding="UTF-8"?>

<Configuration Strict="true">
   <Appenders>
        <Appender type="console" name="con">
            <Layout type="PatternLayout" 
                    pattern="%d[HH:mm:ss.SSS] [%t] %-7level %logger[36] %m%n">
            </Layout>
        </Appender>
      <!-- <Appender type="File" name="file"
                  filename="">   
            <Layout type="PatternLayout" 
                    pattern="%d[HH:mm:ss.SSS] [%t] %-7level %logger[36] %m%n">
            </Layout>
        </Appender>
    -->  
        </Appenders>
   
   <Loggers>
       <Root level="all">
             <AppenderRef ref="con"> 
             </AppenderRef>
       </Root>
   
   </Loggers>
   
</Configuration>